{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "prometheus",
      "description": "",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__elements": {},
  "__requires": [
    {
      "type": "grafana",
      "id": "grafana",
      "name": "Grafana",
      "version": "11.3.0"
    },
    {
      "type": "datasource",
      "id": "prometheus",
      "name": "Prometheus",
      "version": "1.0.0"
    },
    {
      "type": "panel",
      "id": "stat",
      "name": "Stat",
      "version": ""
    },
    {
      "type": "panel",
      "id": "piechart",
      "name": "Pie chart",
      "version": ""
    },
    {
      "type": "panel",
      "id": "barchart",
      "name": "Bar chart",
      "version": ""
    },
    {
      "type": "panel",
      "id": "timeseries",
      "name": "Time series",
      "version": ""
    }
  ],
  "id": null,
  "uid": "stories-execution",
  "title": "Stories Execution",
  "tags": [
    "stories",
    "execution"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 0,
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "story_name",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROMETHEUS}"
        },
        "current": {
          "selected": false,
          "text": "All",
          "value": "$__all"
        },
        "options": [],
        "query": "label_values(dddkit_stories_executions_latency_ms_count, story_name)",
        "refresh": 1,
        "sort": 1,
        "allValue": null,
        "multi": false,
        "includeAll": true,
        "definition": "label_values(dddkit_stories_executions_latency_ms_count, story_name)"
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "Success Rate (%)",
      "type": "stat",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(rate(dddkit_stories_executions_latency_ms_count{status=\"✓\",story_name=~\"$story_name\"}[5m])) / (sum(rate(dddkit_stories_executions_latency_ms_count{story_name=~\"$story_name\"}[5m]))) * 100",
          "refId": "A",
          "legendFormat": "Success Rate",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          }
        }
      ],
      "options": {
        "graphMode": "area",
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "background",
        "reduceOptions": {
          "values": false,
          "fields": "",
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "yellow",
                "value": 50
              },
              {
                "color": "green",
                "value": 90
              }
            ]
          },
          "unit": "percent",
          "custom": {
            "hideFrom": {
              "tooltip": false,
              "viz": false,
              "legend": false
            }
          }
        },
        "overrides": []
      }
    },
    {
      "id": 2,
      "title": "Total Executions",
      "type": "stat",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(increase(dddkit_stories_executions_latency_ms_count{story_name=~\"$story_name\"}[$__range]))",
          "refId": "A",
          "legendFormat": "Total",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          }
        }
      ],
      "options": {
        "graphMode": "none",
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "reduceOptions": {
          "values": false,
          "fields": "",
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          },
          "unit": "short",
          "custom": {
            "hideFrom": {
              "tooltip": false,
              "viz": false,
              "legend": false
            }
          }
        },
        "overrides": []
      }
    },
    {
      "id": 3,
      "title": "Execution Status",
      "type": "piechart",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum by (status) (increase(dddkit_stories_executions_latency_ms_count{story_name=~\"$story_name\"}[$__range]))",
          "refId": "A",
          "legendFormat": "{{ status }}",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          }
        }
      ],
      "options": {
        "reduceOptions": {
          "values": false,
          "fields": "",
          "calcs": [
            "lastNotNull"
          ]
        },
        "pieType": "donut",
        "tooltip": {
          "mode": "single",
          "sort": "none"
        },
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "red",
                "value": 80
              }
            ]
          },
          "custom": {
            "hideFrom": {
              "tooltip": false,
              "viz": false,
              "legend": false
            }
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "✓"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "green"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "✗"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "red"
                }
              }
            ]
          }
        ]
      }
    },
    {
      "id": 4,
      "title": "Success Rate by Story",
      "type": "barchart",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum by (story_name) (increase(dddkit_stories_executions_latency_ms_count{status=\"✓\",story_name=~\"$story_name\"}[$__range])) / sum by (story_name) (increase(dddkit_stories_executions_latency_ms_count{story_name=~\"$story_name\"}[$__range])) * 100",
          "refId": "A",
          "legendFormat": "{{ story_name }}",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          }
        }
      ],
      "options": {
        "legend": {
          "calcs": [],
          "displayMode": "hidden",
          "placement": "bottom"
        },
        "orientation": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "custom": {
            "hideFrom": {
              "tooltip": false,
              "viz": false,
              "legend": false
            }
          }
        },
        "overrides": []
      }
    },
    {
      "id": 5,
      "title": "Latency Percentiles (P50/P95/P99)",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(dddkit_stories_executions_latency_ms_bucket{status=\"✓\",story_name=~\"$story_name\"}[5m])) by (le))",
          "refId": "A",
          "legendFormat": "p50",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          }
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(dddkit_stories_executions_latency_ms_bucket{status=\"✓\",story_name=~\"$story_name\"}[5m])) by (le))",
          "refId": "B",
          "legendFormat": "p95",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          }
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(dddkit_stories_executions_latency_ms_bucket{status=\"✓\",story_name=~\"$story_name\"}[5m])) by (le))",
          "refId": "C",
          "legendFormat": "p99",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          }
        }
      ],
      "options": {
        "legend": {
          "calcs": [
            "mean",
            "max"
          ],
          "displayMode": "table",
          "placement": "right"
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "hideFrom": {
              "tooltip": false,
              "viz": false,
              "legend": false
            }
          }
        },
        "overrides": []
      }
    },
    {
      "id": 6,
      "title": "Success vs Errors Over Time",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "targets": [
        {
          "expr": "sum(increase(dddkit_stories_executions_latency_ms_count{status=\"✓\",story_name=~\"$story_name\"}[5m])) by (story_name)",
          "refId": "A",
          "legendFormat": "{{ story_name }} Success",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          }
        },
        {
          "expr": "sum(increase(dddkit_stories_executions_latency_ms_count{status=\"✗\",story_name=~\"$story_name\"}[5m])) by (story_name)",
          "refId": "B",
          "legendFormat": "{{ story_name }} Errors",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          }
        }
      ],
      "options": {
        "legend": {
          "calcs": [
            "sum",
            "mean"
          ],
          "displayMode": "table",
          "placement": "right"
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "hideFrom": {
              "tooltip": false,
              "viz": false,
              "legend": false
            }
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byRegexp",
              "options": ".*Success.*"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "green"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": ".*Errors.*"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "red"
                }
              }
            ]
          }
        ]
      }
    },
    {
      "id": 7,
      "title": "Average Duration Over Time by Story",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      },
      "targets": [
        {
          "expr": "sum by (story_name, status) (rate(dddkit_stories_executions_latency_ms_sum{story_name=~\"$story_name\"}[5m])) / sum by (story_name, status) (rate(dddkit_stories_executions_latency_ms_count{story_name=~\"$story_name\"}[5m]))",
          "refId": "A",
          "legendFormat": "{{ story_name }} {{ status }}",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          }
        }
      ],
      "options": {
        "legend": {
          "calcs": [
            "mean",
            "max",
            "min"
          ],
          "displayMode": "table",
          "placement": "right"
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "hideFrom": {
              "tooltip": false,
              "viz": false,
              "legend": false
            }
          }
        },
        "overrides": []
      }
    },
    {
      "id": 8,
      "title": "Execution Rate Over Time",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 16
      },
      "targets": [
        {
          "expr": "sum(rate(dddkit_stories_executions_latency_ms_count{story_name=~\"$story_name\"}[1m])) by (story_name)",
          "refId": "A",
          "legendFormat": "{{ story_name }}/min",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          }
        }
      ],
      "options": {
        "legend": {
          "calcs": [
            "mean",
            "max"
          ],
          "displayMode": "table",
          "placement": "right"
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "hideFrom": {
              "tooltip": false,
              "viz": false,
              "legend": false
            }
          }
        },
        "overrides": []
      }
    }
  ]
}