{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "prometheus",
      "description": "",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__elements": {},
  "__requires": [
    {
      "type": "grafana",
      "id": "grafana",
      "name": "Grafana",
      "version": "11.3.0"
    },
    {
      "type": "datasource",
      "id": "prometheus",
      "name": "Prometheus",
      "version": "1.0.0"
    },
    {
      "type": "panel",
      "id": "barchart",
      "name": "Bar chart",
      "version": ""
    },
    {
      "type": "panel",
      "id": "timeseries",
      "name": "Time series",
      "version": ""
    }
  ],
  "id": null,
  "uid": "stories-steps-execution",
  "title": "Stories Steps Execution",
  "tags": [
    "stories",
    "steps"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 0,
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "story_name",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROMETHEUS}"
        },
        "current": {
          "selected": false,
          "text": "All",
          "value": "$__all"
        },
        "options": [],
        "query": "label_values(dddkit_stories_step_executions_latency_ms_count, story_name)",
        "refresh": 1,
        "sort": 1,
        "allValue": null,
        "multi": false,
        "includeAll": true,
        "definition": "label_values(dddkit_stories_step_executions_latency_ms_count, story_name)"
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "Step Duration by Step Name",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum by (step_name, story_name) (dddkit_stories_step_executions_latency_ms_sum{status=\"✓\",story_name=~\"$story_name\"}) / sum by (step_name, story_name) (dddkit_stories_step_executions_latency_ms_count{status=\"✓\",story_name=~\"$story_name\"})",
          "refId": "A",
          "legendFormat": "{{ story_name }} - {{ step_name }}",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          }
        }
      ],
      "options": {
        "legend": {
          "calcs": [
            "mean",
            "max"
          ],
          "displayMode": "table",
          "placement": "right"
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "hideFrom": {
              "tooltip": false,
              "viz": false,
              "legend": false
            }
          }
        },
        "overrides": []
      }
    },
    {
      "id": 2,
      "title": "Step Execution Count Over Time",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum by (step_name, story_name, status) (rate(dddkit_stories_step_executions_latency_ms_count{story_name=~\"$story_name\"}[1m])) * 60",
          "refId": "A",
          "legendFormat": "{{ story_name }} - {{ step_name }} - {{ status }}",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          }
        }
      ],
      "options": {
        "legend": {
          "calcs": [
            "mean"
          ],
          "displayMode": "table",
          "placement": "right"
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "hideFrom": {
              "tooltip": false,
              "viz": false,
              "legend": false
            }
          }
        },
        "overrides": []
      }
    },
    {
      "id": 3,
      "title": "All Steps - P95 Latency Over Time",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(dddkit_stories_step_executions_latency_ms_bucket{status=\"✓\",story_name=~\"$story_name\"}[5m])) by (le, step_name, story_name))",
          "refId": "A",
          "legendFormat": "{{ story_name }} - {{ step_name }}",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          }
        }
      ],
      "options": {
        "legend": {
          "calcs": [
            "mean",
            "max"
          ],
          "displayMode": "table",
          "placement": "right"
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "hideFrom": {
              "tooltip": false,
              "viz": false,
              "legend": false
            }
          }
        },
        "overrides": []
      }
    },
    {
      "id": 4,
      "title": "Step Percentiles Over Time",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 16
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(dddkit_stories_step_executions_latency_ms_bucket{status=\"✓\",story_name=~\"$story_name\"}[5m])) by (le, step_name, story_name))",
          "refId": "A",
          "legendFormat": "{{ story_name }} - {{ step_name }} (p50)",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          }
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(dddkit_stories_step_executions_latency_ms_bucket{status=\"✓\",story_name=~\"$story_name\"}[5m])) by (le, step_name, story_name))",
          "refId": "B",
          "legendFormat": "{{ story_name }} - {{ step_name }} (p95)",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          }
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(dddkit_stories_step_executions_latency_ms_bucket{status=\"✓\",story_name=~\"$story_name\"}[5m])) by (le, step_name, story_name))",
          "refId": "C",
          "legendFormat": "{{ story_name }} - {{ step_name }} (p99)",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          }
        }
      ],
      "options": {
        "legend": {
          "calcs": [
            "mean"
          ],
          "displayMode": "table",
          "placement": "right"
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "hideFrom": {
              "tooltip": false,
              "viz": false,
              "legend": false
            }
          }
        },
        "overrides": []
      }
    },
    {
      "id": 5,
      "title": "Average Step Duration Over Time",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 24
      },
      "targets": [
        {
          "expr": "sum by (step_name, story_name, status) (rate(dddkit_stories_step_executions_latency_ms_sum{story_name=~\"$story_name\"}[5m])) / sum by (step_name, story_name, status) (rate(dddkit_stories_step_executions_latency_ms_count{story_name=~\"$story_name\"}[5m]))",
          "refId": "A",
          "legendFormat": "{{ story_name }} - {{ step_name }} - {{ status }}",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          }
        }
      ],
      "options": {
        "legend": {
          "calcs": [
            "mean",
            "max"
          ],
          "displayMode": "table",
          "placement": "right"
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "hideFrom": {
              "tooltip": false,
              "viz": false,
              "legend": false
            }
          }
        },
        "overrides": []
      }
    },
    {
      "id": 6,
      "title": "Error Count per Step Over Time",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 24
      },
      "targets": [
        {
          "expr": "sum by (step_name, story_name) (increase(dddkit_stories_step_executions_latency_ms_count{status=\"✗\",story_name=~\"$story_name\"}[5m]))",
          "refId": "A",
          "legendFormat": "{{ story_name }} - {{ step_name }}",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          }
        }
      ],
      "options": {
        "legend": {
          "calcs": [
            "sum",
            "mean"
          ],
          "displayMode": "table",
          "placement": "right"
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "hideFrom": {
              "tooltip": false,
              "viz": false,
              "legend": false
            }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 1
              },
              {
                "color": "red",
                "value": 5
              }
            ]
          }
        },
        "overrides": []
      }
    }
  ]
}